{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": "./src",
  "testEnvironment": "node",
  "testRegex": ".spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "~modules/(.*)": "<rootDir>/modules/$1",
    "~queue/(.*)": "<rootDir>/queue/$1"
  }
}
